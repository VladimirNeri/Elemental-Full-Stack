{
  "name": "elemental-full-stack",
  "version": "1.0.0",
  "description": "Elemental Full Stack Application",
  "engines": {
    "node": "14.x"
  },
  "main": "server.js",
  "scripts": {
    "start": "per-env NODE_ENV=production && npm run start:prod || npm run start:development",
    "start:prod": "node server.js",
    "start:development": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Vladimir Neri",
  "license": "ISC",
  "dependencies": {
    "@auth0/auth0-react": "^1.1.0",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.10.1",
    "mongoose": "^5.10.6",
    "nodemailer": "^6.4.14",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "per-env": "^1.0.2",
    "pug": "^3.0.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.6.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
